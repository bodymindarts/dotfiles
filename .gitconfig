    name = Justin Carter
    email = justin.carter@invision.de
[core]
    excludesfile = ~/.gitignore
[init]
    templatedir = ~/.git_template
[alias]
    ctags = !.git/hooks/ctags
    a = add
    rm = remove
    co = checkout
    ci = commit
    b = branch -v
    a = add
    ap = add -p
[credential]
    helper = osxkeychain
